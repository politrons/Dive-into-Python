#!/Users/pabloperezgarcia/Development/DiveIntoPython/venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'mypy-lang==0.5.0','console_scripts','mypy'
__requires__ = 'mypy-lang==0.5.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('mypy-lang==0.5.0', 'console_scripts', 'mypy')()
    )
